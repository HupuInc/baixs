  },
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test mocha -b --reporter dot 'test/**/*.test.js'",
    "deploy": "rsync -rlptD --exclude-from ./.npmignore . $DEPLOY_HOST:/data/www-data/baixs",
