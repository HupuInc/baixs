    "react": "^0.13.3",
    "request": "^2.60.0",
    "shasum": "^1.0.1",
    "swagger-express-mw": "^0.1.0",
    "websocket": "^1.0.21",
    "node-etcd": "^4.1.0",
    "zabbix-node": "^0.2.0",
    "moment": "^2.10.6",
    "nodemailer": "^1.8.0",
    "react-highcharts": "^3.0.0",
    "promise": "^7.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
