{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [1, "single", "avoid-escape"],
    "eol-last": [2],
    "no-trailing-spaces": [2, { "skipBlankLines": true }]
  }
}
